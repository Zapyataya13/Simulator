<Window x:Class="Simulator.GameWindows.LITER"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Simulator.GameWindows"
        mc:Ignorable="d"
        Title="LITER" Height="450" Width="800">
    <Grid>
        <Button x:Name="Chainick" Content="Все об отелях - 800р" HorizontalAlignment="Left" VerticalAlignment="Top" Width="790" Margin="0,20,0,0" Click="Book_Click" />
        <Button x:Name="Sred" Content="Курсы ресторанного бизнеса - 3000р" HorizontalAlignment="Left" Margin="0,50,0,0" VerticalAlignment="Top" Width="790"   Click="Book_Click"/>
        <Button x:Name="vis" Content="Курсы гостеприимства - 8000р" HorizontalAlignment="Left" VerticalAlignment="Top" Width="790" Margin="0,80,0,0"  Click="Book_Click"/>
        <Button x:Name="BOG" Content="Репититор инностранных языков - 9000р" HorizontalAlignment="Left" Margin="0,110,0,0" VerticalAlignment="Top" Width="790"  Click="Book_Click"/>
        <Button x:Name="Closewindow" Content="Закрыть" HorizontalAlignment="Left" Margin="325,330,0,0" VerticalAlignment="Top" Width="153" Height="69" Click="Closewindow_Click"/>
    </Grid>
</Window>
